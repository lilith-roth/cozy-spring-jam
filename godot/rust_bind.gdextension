[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../cozy-spring-jam/target/debug/libcozy_spring_jam.so"
linux.release.x86_64 =   "res://../cozy-spring-jam/target/release/libcozy_spring_jam.so"
windows.debug.x86_64 =   "res://../rust/target/debug/cozy-spring-jam.dll"
windows.release.x86_64 = "res://../rust/target/release/cozy-spring-jam.dll"
macos.debug =            "res://../rust/target/debug/libcozy-spring-jam.dylib"
macos.release =          "res://../rust/target/release/libcozy-spring-jam.dylib"
macos.debug.arm64 =      "res://../rust/target/debug/libcozy-spring-jam.dylib"
macos.release.arm64 =    "res://../rust/target/release/libcozy-spring-jam.dylib"
